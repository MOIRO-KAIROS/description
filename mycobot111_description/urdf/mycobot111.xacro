<?xml version="1.0" ?>
<robot name="mycobot111" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:include filename="$(find mycobot111_description)/urdf/materials.xacro" />
<xacro:include filename="$(find mycobot111_description)/urdf/mycobot111.trans" />
<xacro:include filename="$(find mycobot111_description)/urdf/mycobot111.gazebo" />
<link name="base_link">
  <inertial>
    <origin xyz="-1.3994637957346424e-08 -0.013751224256395762 0.04886454507234413" rpy="0 0 0"/>
    <mass value="12.899764355948896"/>
    <inertia ixx="0.033823" iyy="0.028162" izz="0.035033" ixy="0.0" iyz="-0.000755" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find mycobot111_description)/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find mycobot111_description)/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="link1_1">
  <inertial>
    <origin xyz="0.0068967662247303866 4.702325320090434e-17 0.055490186869056476" rpy="0 0 0"/>
    <mass value="3.4735206294395446"/>
    <inertia ixx="0.004461" iyy="0.004757" izz="0.002424" ixy="0.0" iyz="0.0" ixz="-0.000777"/>
  </inertial>
  <visual>
    <origin xyz="-0.0 -0.0 -0.11" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find mycobot111_description)/meshes/link1_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="-0.0 -0.0 -0.11" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find mycobot111_description)/meshes/link1_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<joint name="joint1" type="continuous">
  <origin xyz="0.0 0.0 0.11" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="link1_1"/>
  <axis xyz="0.0 0.0 1.0"/>
</joint>

</robot>
